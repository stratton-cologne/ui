{
    "name": "@stratton-cologne/ui",
    "version": "1.3.1",
    "type": "module",
    "main": "./dist/index.umd.cjs",
    "module": "./dist/index.es.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.es.js",
            "require": "./dist/index.umd.cjs",
            "default": "./dist/index.es.js"
        },
        "./icon": {
            "types": "./dist/icon.d.ts",
            "import": "./dist/icon.es.js",
            "default": "./dist/icon.es.js"
        },
        "./icons/*": {
            "types": "./dist/icons/*.d.ts",
            "import": "./dist/icons/*.es.js",
            "default": "./dist/icons/*.es.js"
        },
        "./forms": {
            "types": "./dist/entries/forms.d.ts",
            "import": "./dist/forms.es.js",
            "require": "./dist/forms.cjs"
        },
        "./*": {
            "types": "./dist/*.d.ts",
            "import": "./dist/*.es.js",
            "default": "./dist/*.es.js"
        }
    },
    "typesVersions": {
        "*": {
            "forms": [
                "dist/entries/forms.d.ts"
            ],
            "*": [
                "dist/*.d.ts",
                "dist/entries/*.d.ts"
            ]
        }
    },
    "files": [
        "dist"
    ],
    "peerDependencies": {
        "vue": "^3.5.19",
        "vue-router": "^4.5.1",
        "vue-i18n": "^11.1.11"
    },
    "peerDependenciesMeta": {
        "vue-i18n": {
            "optional": true
        }
    },
    "devDependencies": {
        "@vitejs/plugin-vue": "^6.0.1",
        "typescript": "^5.9.2",
        "vite": "^7.1.3",
        "vite-plugin-dts": "^4.5.4",
        "vue": "^3.5.19",
        "vue-router": "^4.5.1",
        "vue-tsc": "^3.0.6",
        "vue-i18n": "^11.1.11"
    },
    "scripts": {
        "prepare:entries": "node scripts/generate-entries.mjs",
        "build:types": "vue-tsc --emitDeclarationOnly --declaration --declarationDir dist",
        "dts:flatten": "node scripts/flatten-dts.mjs",
        "build:esm": "vite build --mode esm",
        "build:umd": "vite build --mode umd",
        "build": "npm run prepare:entries && npm run build:types && npm run dts:flatten && npm run build:esm && npm run build:umd"
    },
    "sideEffects": false
}
